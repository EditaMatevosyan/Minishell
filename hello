hello
pid = fork();
if (pid == 0)
{
    // redirect stdin/stdout
    change_stdin(cmd);
    change_stdout(cmd);

    // execute built-in
    int status = exec_builtin(cmd, &shell->env, shell);
    exit(status); // child exits with status
}
else
{
    waitpid(pid, &g_exit_status, 0);
    if (WIFEXITED(g_exit_status))
        g_exit_status = WEXITSTATUS(g_exit_status);
}
